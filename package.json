{
  "name": "software-testing-assignment",
  "version": "1.0.0",
  "description": "This is a project template for students participating in Software Testing course\r at Tampere University.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "c8 mocha test/**/*.js",
    "coverage": "c8 report --reporter=text-lcov | coveralls"
  },
  "c8": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/.internal/**/*"
    ],
    "all": true,
    "reporter": [
      "text",
      "lcov"
    ],
    "report-dir": "coverage"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "c8": "^8.0.1",
    "chai": "^4.3.10",
    "coveralls": "^3.1.1",
    "mocha": "^10.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.1.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arpitaChiddarwarTUNI/Software-Testing.git"
  },
  "bugs": {
    "url": "https://github.com/arpitaChiddarwarTUNI/Software-Testing/issues"
  },
  "homepage": "https://github.com/arpitaChiddarwarTUNI/Software-Testing#readme"
}
